    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      <!--$description$-->
      C# wrapper for the popular OpenSSL libraries
    </description>
    <releaseNotes>See the list of changes at https://github.com/openssl-net/openssl-net/blob/master/CHANGES </releaseNotes>
    <copyright>Copyright &#169; 2015 OpenSSL Software Foundation</copyright>
    <tags>openssl pki</tags>
  </metadata>
</package>