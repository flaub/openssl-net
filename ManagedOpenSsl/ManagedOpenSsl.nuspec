<?xml version="1.0"?>
<package >
  <!--
    Token replacement comes from:
        $id$	The Assembly name
        $version$	The assembly version as specified in the assemblyâ€™s AssemblyVersionAttribute (or AssemblyInformationalVersionAttribute if present).
        $author$	The company as specified in the AssemblyCompanyAttribute.
        $description$	The description as specified in the AssemblyDescriptionAttribute.
    -->
  
  <metadata>
    <id>$id$$idSuffix$</id>
    <version>$version$$nugetRev$</version>
